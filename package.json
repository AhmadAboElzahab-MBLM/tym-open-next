{
  "name": "tym-open-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-debug": "next build --debug",
    "start": "next start",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "export": "next build && next export",
    "postbuild": "next-sitemap",
    "deploy:en": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -c wrangler.en.jsonc",
    "deploy:en-us": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -c wrangler.en-us.jsonc",
    "deploy:en-ko": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -c wrangler.en-ko.jsonc",
    "deploy:ko": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -c wrangler.ko.jsonc",
    "deploy:router": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -c wrangler.router.jsonc",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@next/third-parties": "^15.1.0",
    "@opennextjs/cloudflare": "^1.14.4",
    "@react-google-maps/api": "^2.20.3",
    "@svgr/webpack": "^8.1.0",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "framer-motion": "^11.11.17",
    "lodash": "^4.17.21",
    "next": "^15.1.9",
    "next-sitemap": "^4.2.3",
    "path": "^0.12.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-modal": "^3.16.1",
    "react-paginate": "^8.2.0",
    "react-places-autocomplete": "^7.3.0",
    "react-select": "^5.8.3",
    "swiper": "11.1.15",
    "use-google-maps-script": "^0.1.2",
    "uuid": "^11.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.9.14",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.17",
    "vercel": "^49.1.1"
  },
  "overrides": {
    "vercel": "$vercel"
  }
}
